Efo_Compounds_ref = Efo_Terms[["EFO Terms","Parent Molecule Name","Max Phase for Indication","References"]]

# Specify the value you want to use for filtering
target_molecule_name = "CANNABIDIOL"
Efo_Term = "schizophrenia"

# Filter the DataFrame based on the target molecule name
filtered_df = Efo_Compounds_ref[(Efo_Compounds_ref['Parent Molecule Name'] == target_molecule_name) &
                               (Efo_Compounds_ref['EFO Terms'] == Efo_Term)]

# Check if there are any matching rows
if not filtered_df.empty:
    # Retrieve the "References" text from the first matching row (assuming there's only one)
    row_text = filtered_df.iloc[0]#["References"]
    references_text = filtered_df.iloc[0]["References"]
    print(row_text)
    print(references_text)
else:
    print(f"No matching rows found for '{target_molecule_name}'")
